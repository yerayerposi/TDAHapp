{
  "name": "rnunitydemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "watch": "node node_modules/typescript/bin/tsc -w",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "release-ios": "react-native unbundle --entry-file index.ios.js --platform ios --dev false",
    "release-android": "node node_modules/typescript/bin/tsc && cd android && ./gradlew assembleRelease",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "react": "16.5.0",
    "react-native": "0.57.0",
    "react-native-camera": "^2.11.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-easy-grid": "^0.2.1",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.0.5",
    "react-native-html-to-pdf": "^0.7.0",
    "react-native-qrcode-scanner": "^1.2.1",
    "react-native-qrcode-svg": "^5.1.2",
    "react-native-qrcosde-scanner": "^1.2.1",
    "react-native-radio-buttons": "^1.0.0",
    "react-native-sqlite-storage": "^3.3.10",
    "react-native-step-modal": "^1.2.1",
    "react-native-svg": "^9.5.1",
    "react-native-unity-view": "1.3.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-view-shot": "^2.6.0",
    "react-navigation": "^3.11.0",
    "victory-native": "^32.0.2"
  },
  "devDependencies": {
    "@types/react": "16.0.36",
    "@types/react-native": "0.52.8",
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "22.2.0",
    "metro-react-native-babel-preset": "0.45.0",
    "jest": "22.2.1",
    "react-test-renderer": "16.2.0",
    "typescript": "^2.7.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
